
## Go to snmp folder
cd D:/Dropbox/12-Office-Sync-MTS/31-WDO/WDO_DV_11A_Bulider/metadata/snmp/

## Turn on port A (-val:1,0,-1,-1,-1,-1,-1,-1)
## Turn off port B
exec SnmpSet.exe -r:192.168.0.200 -c:public -o:.1.3.6.1.4.1.17420.1.2.9.1.13.0 -val:1,0,-1,-1,-1,-1,-1,-1

## Turn on port B (-val:0,1,-1,-1,-1,-1,-1,-1)
## Turn off port A
exec SnmpSet.exe -r:192.168.0.200 -c:public -o:.1.3.6.1.4.1.17420.1.2.9.1.13.0 -val:0,1,-1,-1,-1,-1,-1,-1

## Turn off port A & B both (-val:0,0,-1,-1,-1,-1,-1,-1)
## =====================================================
set fd [exec metadata/snmp/SnmpSet.exe -r:192.168.0.200 -c:public -o:.1.3.6.1.4.1.17420.1.2.9.1.13.0 -val:0,0,-1,-1,-1,-1,-1,-1]
puts $fd
SnmpSet v1.01 - Copyright (C) 2009 SnmpSoft Company
[ More useful network tools on http://www.snmpsoft.com ]

OK
(WDO_DV_11A_Bulider) 120 % 


## Get status
## ==========
exec SnmpGet.exe -r:192.168.0.200 -o:.1.3.6.1.4.1.17420.1.2.9.1.13.0
set fd [exec SnmpGet.exe -r:192.168.0.200 -o:.1.3.6.1.4.1.17420.1.2.9.1.13.0]

set fd [exec metadata/snmp/SnmpGet.exe -r:192.168.0.200 -o:.1.3.6.1.4.1.17420.1.2.9.1.13.0]
puts $fd
SnmpGet v1.01 - Copyright (C) 2009 SnmpSoft Company
[ More useful network tools on http://www.snmpsoft.com ]

OID=.1.3.6.1.4.1.17420.1.2.9.1.13.0
Type=OctetString
Value=0,0,-1,-1,-1,-1,-1,-1
(WDO_DV_11A_Bulider) 121 % 
