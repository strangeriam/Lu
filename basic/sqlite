package require sqlite3

set Database_filename "$::ENVPATH/metadata/trytrylu.db"

sqlite3 db $Database_filename
db timeout 500

db exists "SELECT SerialNumber FROM $StationName WHERE SerialNumber = '$SerialNumber'"
db eval "INSERT INTO TestInfo(SerialNumber,TestStatus,UpdateDateTime) values('$SerialNumber',0,'[time_str]')"
db eval "DELETE FROM $StationName WHERE SerialNumber='$SerialNumber'"

